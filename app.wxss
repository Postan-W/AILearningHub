/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 引入第三方或自定义的字体图标样式。url自己补充 */
@font-face {
    font-family: 'my-icon';
    src: url('./my-icon.ttf') format('truetype');
  }
  
  .my-icon {
    font-family: 'my-icon';
  }
  
  .my-icon-extra::before {
    content: '\e626';
  }
  